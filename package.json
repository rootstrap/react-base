{
  "name": "react_redux_base",
  "version": "0.1.0",
  "description": "Rootstrap React Redux Base",
  "engines": {
    "npm": ">4",
    "node": "10.15.3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel open:src lint",
    "start:server": "node server/build/server.js",
    "ssr": "npm run clean-ssr && npm run build:client && npm run build:server && npm run start:server",
    "heroku-postbuild": "npm run build:client && npm run build:server",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint:code": "eslint webpack/webpack.config.* src tools server --color",
    "lint:style": "stylelint \"src/styles/**/*.scss\"",
    "lint": "npm run lint:code && npm run lint:style",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "clean-ssr": "rimraf ./server/build",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js",
    "build:server": "babel-node server/scripts/buildServer.js",
    "build:client": "babel-node server/scripts/buildClient.js",
    "test": "cypress run",
    "test:open": "cypress open",
    "test:ci": "cypress run --key e83e86fd-9c47-4b2b-980d-517c9aff5974",
    "test:spec": "cypress run --spec",
    "jest": "jest",
    "jest:cover": "npm run jest -- --coverage",
    "jest:watch": "npm run jest -- --watch",
    "deploy": "npm run build && babel-node tools/deployS3",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "author": "Bernabe Gonzalez",
  "license": "MIT",
  "dependencies": {
    "classnames": "2.2.6",
    "connected-react-router": "6.4.0",
    "foundation-sites": "6.4.4-rc1",
    "history": "4.7.2",
    "humps": "2.0.1",
    "immer": "3.1.2",
    "intl": "1.2.5",
    "isomorphic-fetch": "2.2.1",
    "loaders.css": "0.1.2",
    "lodash": "4.17.10",
    "prop-types": "15.6.2",
    "query-string": "5.1.1",
    "react": "16.8.0",
    "react-dom": "16.8.0",
    "react-helmet": "5.2.1",
    "react-intl": "2.4.0",
    "react-loaders": "3.0.1",
    "react-redux": "7.1.0",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.3.1",
    "redux": "4.0.0",
    "redux-form": "8.2.3",
    "redux-logger": "3.0.6",
    "redux-react-session": "2.5.0",
    "redux-thunk": "2.3.0",
    "reselect": "^4.0.0",
    "styled-components": "4.0.2",
    "validate.js": "0.11.1"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "4.0.3",
    "assets-webpack-plugin": "3.9.5",
    "autoprefixer": "9.1.1",
    "aws-sdk": "2.292.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.6.0",
    "babel-loader": "7.1.5",
    "babel-plugin-styled-components": "1.6.4",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.14",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-register": "6.26.0",
    "chalk": "2.4.1",
    "compression": "1.7.3",
    "compression-webpack-plugin": "1.1.11",
    "connect-history-api-fallback": "1.5.0",
    "css-loader": "1.0.0",
    "cypress": "3.3.0",
    "cypress-image-snapshot": "3.0.2",
    "dotenv": "6.0.0",
    "dotenv-webpack": "1.5.7",
    "enzyme": "3.4.1",
    "enzyme-adapter-react-16": "1.5.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-cypress": "2.2.1",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-react-hooks": "1.0.0",
    "express": "4.16.3",
    "faker": "4.1.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "json-loader": "0.5.7",
    "mime": "2.3.1",
    "mini-css-extract-plugin": "0.4.3",
    "nock": "9.6.1",
    "node-sass": "4.12.0",
    "npm-run-all": "4.1.3",
    "opn": "5.3.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-loader": "3.0.0",
    "prompt": "1.0.0",
    "react-hot-loader": "4.6.5",
    "react-test-renderer": "16.5.2",
    "rimraf": "2.6.2",
    "sass-loader": "7.1.0",
    "serialize-javascript": "1.5.0",
    "style-loader": "0.22.1",
    "stylelint": "8.4.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-order": "0.6.0",
    "url-loader": "1.0.1",
    "webpack": "4.19.1",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.3",
    "webpack-md5-hash": "0.0.6",
    "webpack-node-externals": "1.7.2",
    "workbox-webpack-plugin": "3.6.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|ico)$": "<rootDir>/tools/fileMock.js"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/tools/testSetup.js",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/cypress"
    ]
  },
  "keywords": [
    "react",
    "redux",
    "base",
    "starter",
    "kit",
    "rootstrap",
    "boilerplate"
  ],
  "repository": {
    "type": "git",
    "url": ""
  }
}
