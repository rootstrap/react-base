{
  "name": "react_redux_base",
  "version": "0.1.0",
  "description": "Rootstrap React Redux Base",
  "engines": {
    "npm": ">4",
    "node": "10.16.0"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel open:src lint",
    "start:server": "node server/build/server.js",
    "ssr": "npm-run-all clean-ssr build:* start:server",
    "heroku-postbuild": "npm-run-all build:*",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint:code": "eslint webpack/webpack.config.* src tools server --color",
    "lint:style": "stylelint \"src/styles/**/*.scss\"",
    "lint:fix": "eslint --fix webpack/webpack.config.* src",
    "lint": "npm-run-all lint:code lint:style",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "clean-ssr": "rimraf ./server/build",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm-run-all clean-dist lint test",
    "build": "babel-node tools/build.js",
    "build:server": "babel-node server/scripts/buildServer.js",
    "build:client": "babel-node server/scripts/buildClient.js",
    "test": "run-p -r start cypress-run",
    "cypress-run": "cypress run",
    "test:open": "cypress open",
    "test:ci": "cypress run --key e83e86fd-9c47-4b2b-980d-517c9aff5974",
    "test:spec": "cypress run --spec",
    "rm:coverage": "rm -rf coverage || true",
    "check:coverage": "nyc report --reporter=text-summary --reporter=lcov --check-coverage",
    "jest": "jest",
    "jest:cover": "yarn jest -- --coverage",
    "jest:watch": "yarn jest -- --watch",
    "deploy": "yarn build && babel-node tools/deployS3",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
    "prettier": "prettier --write './src/**/*.js'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "nyc": {
    "lines": 70,
    "exclude": [
      "cypress"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "yarn lint:fix --quiet",
      "git add"
    ]
  },
  "author": "Bernabe Gonzalez",
  "license": "MIT",
  "dependencies": {
    "@formatjs/intl-relativetimeformat": "2.6.3",
    "classnames": "2.2.6",
    "connected-react-router": "6.4.0",
    "eslint-plugin-prettier": "3.1.0",
    "foundation-sites": "6.4.4-rc1",
    "history": "4.7.2",
    "humps": "2.0.1",
    "immer": "3.1.2",
    "intl": "1.2.5",
    "isomorphic-fetch": "2.2.1",
    "loaders.css": "0.1.2",
    "lodash": "4.17.10",
    "prop-types": "15.6.2",
    "query-string": "5.1.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-intl": "3.1.8",
    "react-loaders": "3.0.1",
    "react-redux": "7.1.0",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "5.0.1",
    "redux": "4.0.0",
    "redux-form": "8.2.3",
    "redux-logger": "3.0.6",
    "redux-react-session": "2.5.0",
    "redux-thunk": "2.3.0",
    "reselect": "^4.0.0",
    "styled-components": "4.0.2",
    "validate.js": "0.11.1"
  },
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.4",
    "@babel/node": "7.5.0",
    "@babel/plugin-proposal-class-properties": "7.5.0",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-proposal-do-expressions": "7.5.0",
    "@babel/plugin-proposal-export-default-from": "7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "7.5.2",
    "@babel/plugin-proposal-function-sent": "7.5.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.4.4",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "7.5.0",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/plugin-transform-react-constant-elements": "7.5.0",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.4",
    "@cypress/code-coverage": "1.8.0",
    "@cypress/webpack-preprocessor": "4.0.3",
    "@svgr/webpack": "4.3.2",
    "assets-webpack-plugin": "3.9.5",
    "autoprefixer": "9.1.1",
    "aws-sdk": "2.292.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.6",
    "babel-plugin-istanbul": "5.2.0",
    "babel-plugin-styled-components": "1.6.4",
    "babel-plugin-transform-react-remove-prop-types": "0.4.14",
    "chalk": "2.4.1",
    "compression": "1.7.3",
    "compression-webpack-plugin": "1.1.11",
    "connect-history-api-fallback": "1.5.0",
    "core-js": "3.0.1",
    "css-loader": "1.0.0",
    "cypress": "3.3.0",
    "cypress-image-snapshot": "3.0.2",
    "dotenv": "6.0.0",
    "dotenv-webpack": "1.5.7",
    "enzyme": "3.4.1",
    "enzyme-adapter-react-16": "1.5.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-cypress": "2.2.1",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-react-hooks": "1.0.0",
    "express": "4.16.3",
    "faker": "4.1.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.0",
    "identity-obj-proxy": "3.0.0",
    "istanbul-lib-coverage": "2.0.5",
    "jest": "23.6.0",
    "json-loader": "0.5.7",
    "lint-staged": "9.0.2",
    "mime": "2.3.1",
    "mini-css-extract-plugin": "0.4.3",
    "nock": "9.6.1",
    "node-sass": "4.12.0",
    "npm-run-all": "4.1.3",
    "nyc": "14.1.1",
    "opn": "5.3.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-loader": "3.0.0",
    "prettier": "1.18.2",
    "prompt": "1.0.0",
    "react-hot-loader": "4.6.5",
    "react-test-renderer": "16.5.2",
    "rimraf": "2.6.2",
    "sass-loader": "7.1.0",
    "serialize-javascript": "1.5.0",
    "style-loader": "0.22.1",
    "stylelint": "8.4.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-order": "0.6.0",
    "url-loader": "1.0.1",
    "webpack": "4.19.1",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.3",
    "webpack-md5-hash": "0.0.6",
    "webpack-node-externals": "1.7.2",
    "workbox-webpack-plugin": "3.6.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|ico)$": "<rootDir>/tools/fileMock.js"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/tools/testSetup.js",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/cypress"
    ]
  },
  "keywords": [
    "react",
    "redux",
    "base",
    "starter",
    "kit",
    "rootstrap",
    "boilerplate"
  ],
  "repository": {
    "type": "git",
    "url": ""
  }
}
