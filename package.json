{
  "name": "react_redux_base",
  "version": "0.1.0",
  "description": "Rootstrap React Redux Base",
  "engines": {
    "npm": ">4",
    "node": ">6"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel open:src lint",
    "start:server": "node server/build/server.js",
    "ssr": "npm run clean-dist && npm run build:client && npm run build:server && npm run start:server",
    "heroku-postbuild": "npm run build:client && npm run build:server",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint:code": "eslint webpack.config.* src tools server --color",
    "lint:style": "stylelint \"src/styles/**/*.scss\"",
    "lint": "npm run lint:code && npm run lint:style",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js",
    "build:server": "babel-node server/scripts/buildServer.js",
    "build:client": "babel-node server/scripts/buildClient.js",
    "test": "jest",
    "test:cover": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "deploy": "npm run build && babel-node tools/deployS3",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "author": "Bernabe Gonzalez",
  "license": "MIT",
  "dependencies": {
    "classnames": "2.2.6",
    "foundation-sites": "6.4.4-rc1",
    "history": "4.7.2",
    "humps": "2.0.1",
    "immutable": "3.8.2",
    "intl": "1.2.5",
    "isomorphic-fetch": "2.2.1",
    "loaders.css": "0.1.2",
    "lodash": "4.17.10",
    "prop-types": "15.6.1",
    "query-string": "5.1.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-helmet": "5.2.0",
    "react-intl": "2.4.0",
    "react-loaders": "3.0.1",
    "react-redux": "5.0.7",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "4.0.0",
    "redux-form": "7.3.0",
    "redux-immutable": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-react-session": "2.4.0",
    "redux-thunk": "2.2.0",
    "validate.js": "0.11.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.5.1",
    "autoprefixer": "8.4.1",
    "aws-sdk": "2.243.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-jest": "22.4.3",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-register": "6.26.0",
    "chalk": "2.3.0",
    "compression": "1.7.2",
    "compression-webpack-plugin": "1.1.11",
    "connect-history-api-fallback": "1.5.0",
    "css-loader": "0.28.1",
    "dotenv": "5.0.1",
    "dotenv-webpack": "1.5.5",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "express": "4.16.3",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "22.4.3",
    "json-loader": "0.5.7",
    "mime": "2.3.1",
    "nock": "9.2.5",
    "node-sass": "4.9.0",
    "npm-run-all": "4.1.2",
    "opn": "5.3.0",
    "postcss-loader": "2.1.4",
    "prompt": "1.0.0",
    "react-hot-loader": "4.1.2",
    "react-test-renderer": "16.3.2",
    "redux-immutable-state-invariant": "2.0.0",
    "replace": "0.3.0",
    "rimraf": "2.6.2",
    "sass-loader": "7.0.1",
    "serialize-javascript": "1.5.0",
    "style-loader": "0.21.0",
    "stylelint": "8.4.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-order": "0.6.0",
    "url-loader": "1.0.1",
    "webpack": "4.6.0",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.1",
    "webpack-md5-hash": "0.0.6",
    "webpack-node-externals": "1.7.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|ico)$": "<rootDir>/tools/fileMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/tools/testSetup.js"
  },
  "keywords": [
    "react",
    "redux",
    "base",
    "starter",
    "kit",
    "rootstrap",
    "boilerplate"
  ],
  "repository": {
    "type": "git",
    "url": ""
  }
}
